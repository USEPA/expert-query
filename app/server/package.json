  "dependencies": {
    "@aws-sdk/client-s3": "3.428.0",
    "axios": "1.5.1",
    "browser-sync": "2.29.3",
    "cors": "2.8.5",
    "dotenv": "16.3.1",
    "exceljs": "4.3.0",
    "express": "4.18.2",
    "express-basic-auth": "1.2.1",
    "helmet": "7.0.0",
    "knex": "3.0.1",
    "log4js": "6.9.1",
    "papaparse": "5.4.1",
    "pg": "8.11.3",
    "pg-query-stream": "4.5.3",
    "serve-favicon": "2.5.0"
  },
  "devDependencies": {
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.1",
    "husky": "8.0.3",
    "lint-staged": "14.0.1",
    "nodemon": "3.0.1",
    "prettier": "3.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "app/**/*.{js,json,css,md}": "prettier --write"
  },
  "type": "module"
}
